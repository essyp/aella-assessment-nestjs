  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm:cli migration:create",
    "migration:generate": "npm run typeorm:cli migration:generate  -- -d src/shared/typeorm/migration.datasource.ts",
    "migration:regenerate": "npm run typeorm:cli migration:generation",
    "migration:run": "npm run typeorm:cli migration:run --  -d src/shared/typeorm/migration.datasource.ts",
    "migrate": "typeorm migration:run -d dist/shared/typeorm/migration.datasource.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
